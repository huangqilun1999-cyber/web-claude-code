{
  "name": "web-claude-code",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true })\"",
    "db:push": "pnpm --filter web db:push",
    "db:migrate": "pnpm --filter web db:migrate",
    "db:studio": "pnpm --filter web db:studio",
    "typecheck": "turbo run typecheck",
    "dev:ws": "pnpm --filter ws-server dev",
    "dev:agent": "pnpm --filter wcc-agent dev",
    "build:shared": "pnpm --filter @wcc/shared build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0"
  }
}
